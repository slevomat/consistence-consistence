{
	"name": "slevomat/consistence",
	"description": "Consistence - consistent approach and additions to PHP's functionality",
	"keywords": ["enum", "array", "time", "format", "strict", "type", "check"],
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"php": "^7.2.0 || ^8.0.0"
	},
	"require-dev": {
		"consistence/coding-standard": "dev-feature/php-8-support",
		"phing/phing": "2.16.3",
		"php-parallel-lint/php-console-highlighter": "0.5.0",
		"php-parallel-lint/php-parallel-lint": "1.2.0",
		"phpunit/phpunit": "8.5.2 || 9.3.8"
	},
	"autoload": {
		"psr-4": { "Consistence\\": [ "src" ] },
		"classmap": [ "src" ]
	},
	"autoload-dev": {
		"psr-4": { "Consistence\\": [ "tests" ] },
		"classmap": [ "tests" ]
	},
	"config": {
		"bin-dir": "bin"
	},
	"replace": {
		"consistence/consistence": "*"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/Th3Mouk/coding-standard.git"
		}
	]
}
